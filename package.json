{
  "name": "paseo_contact_us",
  "version": "0.0.1",
  "description": "Paseo Contact Us Form",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "npm-run-all -n -p prod:server",
    "dev": "npm-run-all -n -p dev:server lint:watch",
    "dev:watch": "webpack -w -d",
    "prod:server": "cross-env NODE_ENV=production webpack-dev-server --color --progress",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --color --progress --hot",
    "build": "npm-run-all -s build:webpack",
    "dev:build": "cross-env NODE_ENV=development npm-run-all -s webpack:dev",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "gh-deploy": "gh-pages -d dist",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catenare/paseo-contact-us"
  },
  "keywords": [
    "Static",
    "webpack",
    "theme"
  ],
  "author": "Johan Martin <martin.johan@johan-martin.com> (http://www.johan-martin.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catenare/paseo-contact-us/issues"
  },
  "homepage": "https://github.com/catenare/paseo-contact-us",
