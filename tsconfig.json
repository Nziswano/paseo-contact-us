{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "jsx": "preserve",
    "strict": true,
    "allowJs": true,
    "pretty": true,
    "diagnostics": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules"
  ]
}
